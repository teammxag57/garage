generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model ShopSession {
  shop        String   @id              // ex: "team-mx-ag57.myshopify.com"
  accessToken String
  scopes      String?
  updatedAt   DateTime @updatedAt
  createdAt   DateTime @default(now())
}
